                        />
                      </div>
                    </div>
                    <div class="col-md-6 p-sm-0">
                      <div class="form-group">
                        <label for="inputLname">Truck Plate No.</label>
                        <input
                          type="text"
                          class="form-control"
                          id="inputLname"
                          placeholder="e.g.  Robert"
                        />
                      </div>
                    </div>
                  </div>

                  <div class="form-group">
                    <label for="remark">Driver Contact</label>
                    <input
                      type="text"
                      class="form-control"
                      id="inputLname"
                      placeholder="Driver Contact"
                    />
                  </div>
                  <div class="form-group">
                    <label for="remark">Remark form</label>
                    <textarea
                      class="form-control"
                      name=""
                      id="remark"
                      cols="30"
                      rows="10"
                    ></textarea>
                  </div>
