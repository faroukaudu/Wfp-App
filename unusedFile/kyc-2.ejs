    <div class="ugf-bg ufg-main-container">
      <div class="ugf-progress">
        <div class="progress">
          <div
            class="progress-bar"
            role="progressbar"
            style="width: 66.66%; background-color: #009bb6"
            aria-valuenow="66.66"
            aria-valuemin="0"
            aria-valuemax="100"
          ></div>
        </div>
      </div>

      <div class="container">
        <div class="row">
          <div class="col-lg-6 offset-lg-3 p-sm-0">
            <div class="ugf-form">
              <form action="kyc-3.html">
                <div class="input-block">
                  <div class="form-group">
                    <div class="form-group">
                      <label for="Date">Commodity</label>
                      <input
                        type="text"
                        class="form-control"
                        id="Date"
                        placeholder="Commodity"
                      />
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-md-6 p-sm-0">
                      <div class="form-group">
                        <label for="Date">Unit Weight</label>
                        <input
                          type="text"
                          class="form-control"
                          id="Date"
                          placeholder="e.g.  Smith"
                        />
                      </div>
                    </div>
                    <div class="col-md-6 p-sm-0">
                      <div class="form-group">
                        <label for="Date">Quantity</label>
                        <input
                          type="text"
                          class="form-control"
                          id="Date"
                          placeholder="e.g.  Smith"
                        />
                      </div>
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-md-6 p-sm-0">
                      <div class="form-group">
                        <label for="Date">Metri-ton</label>
                        <input
                          type="text"
                          class="form-control"
                          id="Date"
                          placeholder="e.g.  Smith"
                        />
                      </div>
                    </div>
                    <div class="col-md-6 p-sm-0">
                      <div class="form-group">
                        <label for="Date">Activity</label>
                        <input
                          type="text"
                          class="form-control"
                          id="Date"
                          placeholder="e.g.  Smith"
                        />
                      </div>
                    </div>
                  </div>
                  <h4 style="text-align: center">Destination</h4>
                  <div class="row">
                    <div class="col-md-6 p-sm-0">
                      <div class="form-group">
                        <label for="Date">From</label>
                        <input
                          type="text"
                          class="form-control"
                          id="Date"
                          placeholder="e.g.  Smith"
                        />
                      </div>
                    </div>
                    <div class="col-md-6 p-sm-0">
                      <div class="form-group">
                        <label for="Date">To</label>
                        <input
                          type="text"
                          class="form-control"
                          id="Date"
                          placeholder="e.g.  Smith"
                        />
                      </div>
                    </div>
                  </div>
                  <button class="btn">Next Step &nbsp; &#10140;</button>
                </div>
              </form>
              <a href="kyc.html" class="back-to-prev"
                ><img src="assets/images/arrow-left-grey.png" alt="" /> Back to
                Previous</a
              >
            </div>
          </div>
        </div>
      </div>
    </div>

    <footer class="">
