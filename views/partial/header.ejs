<!DOCTYPE html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1, shrink-to-fit=no"
    />

    <title>Goldpark logistics</title>

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="assets/css/bootstrap.min.css" />
    <!-- <link rel="stylesheet" href="assets/css/bootstrap.min.css"> -->
    
    <!-- Style -->
    <link rel="stylesheet" href="assets/css/style.css">

    <!-- External Css -->
    <link rel="stylesheet" href="assets/css/line-awesome.min.css" />
    <link rel="stylesheet" href="assets/css/countrySelect.css" />

    <!-- Custom Css -->
    <link rel="stylesheet" type="text/css" href="assets/css/main.css" />
    <link rel="stylesheet" type="text/css" href="assets/css/kyc-1.css" />
    <link rel="stylesheet" type="text/css" href="assets/css/covid.css" />
    <link rel="stylesheet" type="text/css" href="assets/css/style1.css" />

    <!-- Fonts -->
    <link
      href="https://fonts.googleapis.com/css2?family=Roboto:wght@400;500;700&display=swap"
      rel="stylesheet"
    />

    <!-- Favicon -->
    <link rel="icon" href="assets/images/favicon.png" />
    <link rel="apple-touch-icon" href="assets/images/apple-touch-icon.png" />
    <link
      rel="apple-touch-icon"
      sizes="72x72"
      href="assets/images/icon-72x72.png"
    />
    <link
      rel="apple-touch-icon"
      sizes="114x114"
      href="assets/images/icon-114x114.png"
    />
  </head>
  <body>
    <div class="ugf-nav">
      <div class="container">
        <div class="row">
          <div class="col">
            <div class="navigation">
              <div class="logo">
                <a href="#"
                  ><img
                    src="assets/images/Logo/isadora std regula font - Copy.png"
                    class="img-fluid"
                    width="130"
                    alt=""
                /></a>
              </div>
              <div class="nav-btns">
                <h3 class="text-right back">
                  <span class="fw-bolder text-uppercase"> <%= userInfo.fullname%></span
                  ><br />
                  <span class="text-right text-primary"><%= userInfo.position%></span><br />
                  <em style="font-size: 12px; color: rgb(145, 144, 144)"
                    ><%= userInfo.email%></em
                  >
                </h3>
                <img
                  style="border-radius: 50%; border: 1px solid rgb(79, 77, 77)"
                  src="login/img/profile.png"
                  width="50px"
                  alt=""
                />
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="assets/js/jquery.min.js"></script>
    <script src="assets/js/popper.min.js"></script>
    <script src="assets/js/bootstrap.min.js"></script>
    <script src="assets/js/reg.js"></script>

    <script src="assets/js/countrySelect.min.js"></script>

    <script src="assets/js/custom.js"></script>

